type ApplicationsConnection {
  edges: [ApplicationEdge]
  totalCount: Int!
  pageInfo: PageInfo
}

type ApplicationEdge {
  cursor: String
  node: Application
}

type Application {
  affiliation: Affiliation!
  environment: String
  namespace: Namespace
  name: String!
  status: Status!
  version: Version!
}

type Status {
  code: String!
  comment: String
}

type Version {
  deployTag: String
  auroraVersion: String
}
