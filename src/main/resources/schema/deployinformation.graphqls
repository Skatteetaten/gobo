type DeployInformation {
    deployDetail: [DeployDetail]!
}

type DeployDetail {
    cluster: String!
    environment: String!
    application: String!
    version: String
    replicas:String
    type: String
    deploy_strategy:String
}

input DeployInformationInput {
    auroraConfigName: String!
    auroraConfigReference: String!
    path : [String!]!
}
