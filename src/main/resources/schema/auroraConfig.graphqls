type AuroraConfig{
    name: String!
    ref: String!
    resolvedRef: String!
    version: String!
    files(types:[AuroraConfigFileType!], fileName:[String!]): [AuroraConfigFile!]
}

type AuroraConfigFile {
    name: String!
    type: AuroraConfigFileType!
    contents: String!
}

enum AuroraConfigFileType  {
    GLOBAL,
    BASE,
    ENV,
    APP
}