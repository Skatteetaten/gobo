type GitInfo {
  commitId: String
  commitTime: String
}

type ImageDetails {
  imageBuildTime: String
  dockerImageReference: String
}

type PodResource {
  name: String
  status: String
  restartCount: Int
  ready: Boolean
  startTime: String
  links(names: [String]): [Link]
}

type Link {
  name: String
  url: String
}

type ApplicationInstanceDetails {
  buildTime: String
  gitInfo: GitInfo
  imageDetails: ImageDetails
  podResources: [PodResource]
}
